<?xml version="1.0" encoding="UTF-8"?>
<Module>
	<Name>CyclePeakLookahead</Name>
	<Vendor>YourNameOrCompany</Vendor>
	<Description>
		Audio module that delays the signal up to 30 ms and provides a lookahead
		peak detection output with optional absolute mode and hysteresis.
	</Description>

	<Pin name="Input" datatype="float" direction="in" rate="audio" />
	<Pin name="Output" datatype="float" direction="out" rate="audio" />
	<Pin name="Peak" datatype="float" direction="out" rate="control" />
	<Pin name="Lookahead (ms)" datatype="float" direction="in" default="5" />
	<Pin name="Hysteresis" datatype="float" direction="in" default="0.001" />
	<Pin name="Absolute Mode" datatype="int" direction="in" default="0" />
</Module>
